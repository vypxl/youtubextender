{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "YoutubeXTender",
  "description": "Some minor quality-of-life features for the YouTube website.",
  "host_permissions": ["http://youtube.com/*", "https://youtube.com/*"],
  "content_scripts": [
    {
      "matches": [
        "http://youtube.com/*",
        "https://youtube.com/*",
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "js": ["timeWidget.js", "player.js", "subs.js"]
    }
  ]
}
